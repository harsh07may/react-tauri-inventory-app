{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-load",
    "sql:allow-select",
    "sql:allow-close",
    "dialog:default",
    "fs:default",
    "fs:allow-copy-file",
    "fs:allow-exists",
    "fs:allow-write-file",
    "fs:allow-appdata-read-recursive",
    "fs:allow-appdata-write-recursive"
  ]
}
